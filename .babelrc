{
  "presets": [
    ["env", {
      "modules": "umd"
    }],
    ["stage-3"],
  ],
  "plugins": ["transform-runtime", "lodash"],
  "env": {
    "production": {
      "ignore": ".spec.js",
      "plugins": [
        ["module-resolver", {
          "root": ["./src"]
        }]
      ],
    },
    "test": {
      "plugins": [
        ["module-resolver", {
          "root": ["./src", "../../misc"]
        }]
      ]
    }
  }
}
